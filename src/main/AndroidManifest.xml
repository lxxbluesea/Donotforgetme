<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.donotforgetme">

    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <!--<uses-permission android:name="android.permission.WRITE_SMS"/>-->
    <!--<uses-permission android:name="android.permission.RECEIVE_SMS"/>-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_CALL_LOG"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.example.donotforgetme.Utils.ApplicationUtil"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar">
        <!--android:theme="@style/AppTheme" >-->
        <activity
            android:name="com.example.donotforgetme.Activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Normal_Executing"
            android:label="@string/title_activity_executing"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Normal_Delay"
            android:label="@string/title_activity_delay"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Normal_Delete"
            android:label="@string/title_activity_delete"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Normal_Finish"
            android:label="@string/title_activity_finish"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Setting_Normal"
            android:label="@string/title_activity_setting_normal"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Setting_Warn"
            android:label="@string/title_activity_setting_warn"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Setting_Backup"
            android:label="@string/title_activity_setting_backup"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Add"
            android:label="@string/title_activity_add"></activity>
        <activity
            android:name="com.example.donotforgetme.Activities.Help"
            android:label="@string/title_activity_help"></activity>
    </application>

</manifest>
