    <array name="initSql">
        <item>CREATE TABLE BackupType (
            id INT PRIMARY KEY,
            name VARCHAR( 20 )
            );</item>
        <item>CREATE TABLE Item (
            ID INT PRIMARY KEY,
            content VARCHAR( 500 ),
            level INT,
            createDatetime INTEGER,
            beginDatetime INTEGER,
            endDatetime INTEGER,
            noticetime INT
            );</item>
        <item>CREATE TABLE ItemNotice (
            ID INTEGER PRIMARY KEY AUTOINCREMENT,
            itemID INT,
            noticeid INT,
            noticetime INTEGER
            );</item>
        <item>CREATE TABLE ItemStatus (
            ID INTEGER PRIMARY KEY AUTOINCREMENT,
            itemid INT,
            statusid INT,
            opdatetime INTEGER,
            note VARCHAR( 200 )
            );</item>
        <item>CREATE TABLE Notice (
            ID INT PRIMARY KEY,
            name VARCHAR( 20 ),
            enable INT,
            ringpath VARCHAR( 200 ),
            zhendongnum INT
            );</item>
        <item>CREATE TABLE Status (
            ID INT PRIMARY KEY,
            name VARCHAR( 20 )
            );</item>
        <item>CREATE TABLE Submit (
            id INTEGER PRIMARY KEY AUTOINCREMENT,
            title VARCHAR( 200 ),
            content VARCHAR( 500 ),
            contact VARCHAR( 20 )
            );</item>
        <item>CREATE TABLE Backup (
            id INTEGER PRIMARY KEY AUTOINCREMENT,
            backuptypeid INT,
            list VARCHAR( 200 ),
            backupdir VARCHAR( 200 )
            );</item>
        <item>CREATE TABLE BaseInfo (
            id INTEGER PRIMARY KEY AUTOINCREMENT,
            number VARCHAR( 20 ),
            shownum INT,
            showday INT,
            backupdir VARCHAR( 200 ),
            mailtype VARCHAR( 20 ),
            mailname VARCHAR( 20 ),
            mailpassword VARCHAR( 20 ),
            smtp VARCHAR( 20 ),
            pop3 VARCHAR( 20 ),
            testmailresult INT
            );</item>
        <item>CREATE TABLE Permission (
            ID INT PRIMARY KEY,
            name VARCHAR( 20 ),
            Enable INT
            );</item>
        <item>            CREATE TABLE Level (
            id INT PRIMARY KEY,
            name VARCHAR( 20 )
            );</item>
    </array>
